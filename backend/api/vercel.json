{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/health$", "dest": "/health.js" },
    { "src": "^/product_list$", "dest": "/product_list.js" },
    { "src": "^/product(.*)$", "dest": "/product.js" },
    { "src": "^/user(.*)$", "dest": "/user.js" },
    { "src": "^/api/(.*)$", "dest": "/[...slug].js" },
    { "src": "^/(.*)$", "dest": "/[...slug].js" }
  ]
}
